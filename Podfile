  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for BubblePond
  pod 'AudioKit', '~> 4.7'
  pod 'SwiftLint'
  pod 'CocoaLumberjack/Swift'

  target 'BubblePondTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'BubblePondUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end